{
  "expo": {
    "name": "smart_home",
    "slug": "smart_home",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#F2E6E1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#28AF62"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
